# DSO API versioning; see the README which is nearby.
# else https://wiki.scold-lang.org/page/DSO
#
#   CURRENT <= CURRENT+1 if any interfaces were added
#           <= CURRENT+1 if any interfaces were removed
#           <= CURRENT+1 if any interfaces were changed
#           <= CURRENT   if no interface change, but any other (source code) change
#
#   REVISION <= 0          if any interfaces were added
#            <= 0          if any interfaces were removed
#            <= 0          if any interfaces were changed
#            <= REVISION+1 if no interface change, but any other (source code) change
#
#   AGE <= AGE+1 if any interaces were added
#       <= 0     if any interfaces were removed
#       <= 0     if any interfaces were changed
#       <= AGE   if no interface change, but any other (source code) change
#
lib_libhyperledger_fabric_user_la_CURRENT = $(lib_libhyperledger_la_CURRENT)
lib_libhyperledger_fabric_user_la_REVISION = $(lib_libhyperledger_la_REVISION)
lib_libhyperledger_fabric_user_la_AGE = $(lib_libhyperledger_la_AGE)
lib_libhyperledger_fabric_user_la_CRA = $(lib_libhyperledger_fabric_user_la_CURRENT):$(lib_libhyperledger_fabric_user_la_REVISION):$(lib_libhyperledger_fabric_user_la_AGE)

lib_libhyperledger_fabric_user_la = lib/libhyperledger-fabric-user.la
lib_libhyperledger_fabric_user_la_LDFLAGS = $(PACKAGE_LDFLAGS_SET) -version-info $(lib_libhyperledger_fabric_user_la_CRA)
# find the subsystem declarations in configure.ac driving the -l and -l*-module mentions
lib_libhyperledger_fabric_user_la_LIBADD = \
  $(lib_libwant_la) \
  -ltunitas \
  -lnonstd-module \
  $(end)
lib_libhyperledger_fabric_user_la_MODULES = $(call TF_SOURCEStoMODULES, $(lib_libhyperledger_fabric_user_la_SOURCES))
lib_libhyperledger_fabric_user_la_SOURCES = \
  obj/src/hyperledger/fabric/user/namespace.cpp \
  obj/src/hyperledger/fabric/user/packaging/namespace.cpp \
  \
  obj/src/hyperledger/fabric/User.cpp \
  obj/src/hyperledger/fabric/user/User.cpp \
  \
  $(obj_srv_hyperledger_fabric_user_create_SUBSOURCES) \
  $(obj_src_hyperledger_fabric_user_error_SUBSOURCES) \
  $(obj_src_hyperledger_fabric_user_exception_SUBSOURCES) \
  $(obj_src_hyperledger_fabric_user_load_SUBSOURCES) \
  obj/src/hyperledger/fabric/user/validate.cpp \
  $(end)
obj_src_hyperledger_fabric_user_create_SUBSOURCES = \
  obj/src/hyperledger/fabric/user/create.cpp \
  obj/src/hyperledger/fabric/user/create_user.cpp \
  obj/src/hyperledger/fabric/user/packaging/create/namespace.cpp \
  obj/src/hyperledger/fabric/user/packaging/create/interface.cpp \
