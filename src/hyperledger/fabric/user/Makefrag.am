  $(end)
user-objects : $(patsubst %.cpp, %.lo, $(lib_libhyperledger_fabric_user_la_SOURCES))
include src/hyperledger/fabric/user/error/Makefrag.am
include src/hyperledger/fabric/user/exception/Makefrag.am
$(lib_libhyperledger_fabric_user_la_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/hyperledger/fabric/user/dependencies.mk:
