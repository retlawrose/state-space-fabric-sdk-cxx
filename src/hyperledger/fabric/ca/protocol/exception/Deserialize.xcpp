#divert <fpp>
namespace hyperledger::fabric::ca::protocol::exception {
  //
  // There is some problem with a deserialization activity.
  //
  // Concept:
  //
  //   This is classed as a runtime_error because it is related to the data-from-the-wire.
  //
  //   The reason must be a terse explanation.  Not too flowery.
  //   Be cognizant that spewing incorrect values upwards through the error recovery channel is problematic.
  //
  // Usage:
  //
  //   throw exception::Deserialize{"zero length array"};
  //
  class Deserialize;
}
#endiv
#divert <hpp>
#import hyperledger.fabric.ca.protocol.exception.Aspects
#import hyperledger.string.Storage
#import std.runtime_error
class hyperledger::fabric::ca::protocol::exception::Deserialize : public protocol::exception::Aspects,
                                                                  public std::runtime_error {
  struct Ancestors {
    using Aspects = protocol::exception::Aspects;
    using Exception = std::runtime_error;
  };
public:
  inline explicit Deserialize(string::Storage const &reason);
  auto what() const noexcept -> char const * override;
};
#endiv
#divert <ipp>
namespace hyperledger::fabric::ca::protocol::exception {
  Deserialize::Deserialize(string::Storage const &reason)
    : Ancestors::Exception{reason}
  { }
}
#endiv
#divert <cpp>
namespace hyperledger::fabric::ca::protocol::exception {
  auto Deserialize::what() const noexcept -> char const * { return Ancestors::Exception::what(); }
}
#endiv
