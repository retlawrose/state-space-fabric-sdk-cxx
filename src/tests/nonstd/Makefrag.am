check_bin_nonstd = check/bin/nonstd
check_bin_nonstd_LDADD = \
  $(lib_libwant_nonstd_la) \
  -ltunitas \
  -lrigging-bench-module \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_nonstd_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_nonstd_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_nonstd_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_nonstd_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_nonstd_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_nonstd_SOURCES))
check_bin_nonstd_SOURCES = \
  obj/src/tests/nonstd/main.cpp \
  obj/src/tests/nonstd/namespace.cpp \
  obj/src/tests/nonstd/filesystem.Fixture.cpp \
  obj/src/tests/nonstd/filesystem.dequote.Fixture.cpp \
  obj/src/tests/nonstd/required.Fixture.cpp \
  obj/src/tests/nonstd/units.Fixture.cpp \
  obj/src/tests/nonstd/units.Count.cpp \
  $(end)
$(check_bin_nonstd_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/nonstd/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_bin_nonstd_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/nonstd/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/nonstd/dependencies.mk
