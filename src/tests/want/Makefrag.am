check_bin_want = check/bin/want
check_bin_want_LDADD = \
  $(lib_libwant_la) \
  -ltunitas \
  -lrigging-bench-module \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_want_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_want_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_want_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_want_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_want_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_want_SOURCES))
check_bin_want_SOURCES = \
  obj/src/tests/want/main.cpp \
  obj/src/tests/want/namespace.cpp \
  obj/src/tests/want/Fixture.cpp \
  $(end)
$(check_bin_want_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/want/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_bin_want_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/want/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/want/dependencies.mk
