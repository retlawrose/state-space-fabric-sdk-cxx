#divert <cpp>
#import std.string_literals
using namespace std::string_literals;
#import want.Boolean
auto tests::unit::want::Fixture::test_Boolean() -> void {
  using ::want::Boolean;
  Boolean b0;
  CPPUNIT_ASSERT(false == b0);
  Boolean b1{true};
  CPPUNIT_ASSERT(true == b1);
  Boolean b2;
  b2 = b1;
  CPPUNIT_ASSERT(true == b2);
  b2 = b0;
  CPPUNIT_ASSERT(false == b2);
}
