check_bin_slurp = check/bin/slurp
check_bin_slurp_LDADD = \
  $(lib_libhyperledger_la) \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_slurp_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_slurp_LDFLAGS = $(CHECK_LDFLAGS_SET)
#(check_bin_slurp_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
#(check_bin_slurp_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_slurp_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_slurp_SOURCES))
check_bin_slurp_SOURCES = \
  obj/src/tests/slurp/main.cpp \
  obj/src/tests/slurp/namespace.cpp \
  $(end)
$(check_bin_slurp_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/slurp/dependencies.mk:
	$(TF_V_DIS) $(call TF_DEPENDENCIES, $(check_bin_slurp_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/slurp/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/slurp/dependencies.mk
