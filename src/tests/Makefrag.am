check_lib_libtests_la = check/lib/libtests.la
check_lib_libtests_la_LIBADD = $(nothing)
check_lib_libtests_la_MODULES = $(call TF_SOURCEStoMODULES, $(check_lib_libtests_la_SOURCES))
check_lib_libtests_la_SOURCES = \
  obj/src/tests/namespace.cpp \
  $(end)
tests-objects : $(patsubst %.cpp, %.lo, $(check_lib_libtests_la_SOURCES))
$(check_lib_libtests_la_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_lib_libtests_la_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/dependencies.mk
