check_bin_shell = check/bin/shell
check_bin_shell_LDADD = \
  $(lib_libwant_shell_la) \
  -ltunitas \
  -lrigging-bench-module \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_shell_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_shell_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_shell_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_shell_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_shell_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_shell_SOURCES))
check_bin_shell_SOURCES = \
  obj/src/tests/shell/main.cpp \
  obj/src/tests/shell/namespace.cpp \
  obj/src/tests/shell/run.Fixture.cpp \
  obj/src/tests/shell/run.Fixture.Runner.cpp \
  obj/src/tests/shell/run.Fixture.Piped_From.cpp \
  obj/src/tests/shell/run.Fixture.Piped_To.cpp \
  $(end)
$(check_bin_shell_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/shell/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_bin_shell_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/shell/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/shell/dependencies.mk
