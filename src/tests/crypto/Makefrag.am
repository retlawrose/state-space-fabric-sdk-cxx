check_bin_unit_crypto = check/bin/unit-crypto
check_bin_unit_crypto_LDADD = \
  $(lib_libhyperledger_la) \
  $(lib_libhyperledger_fabric_la) \
  $(lib_libhyperledger_fabric_ca_la) \
  $(lib_libhyperledger_fabric_user_la) \
  $(lib_libwant_la) \
  -ltunitas \
  -lrigging-bench-module \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_unit_crypto_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_unit_crypto_LDFLAGS = $(AND_CHECK_LDFLAGS_SET)
#(check_bin_unit_crypto_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
#(check_bin_unit_crypto_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_unit_crypto_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_unit_crypto_SOURCES))
check_bin_unit_crypto_SOURCES = \
  obj/src/tests/crypto/main.cpp \
  obj/src/tests/crypto/namespace.cpp \
  \
  obj/src/tests/crypto/Fixture.cpp \
  obj/src/tests/crypto/curve.Fixture.cpp \
  obj/src/tests/crypto/ecies.Fixture.cpp \
  obj/src/tests/crypto/hash.Fixture.cpp \
  obj/src/tests/crypto/key.Fixture.cpp \
  obj/src/tests/crypto/number.Fixture.cpp \
  obj/src/tests/crypto/number.Number.cpp \
  obj/src/tests/crypto/number.Comparison.cpp \
  obj/src/tests/crypto/number.Expression.cpp \
  $(end)
$(check_bin_unit_crypto_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/crypto/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_bin_unit_crypto_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/crypto/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/crypto/dependencies.mk
