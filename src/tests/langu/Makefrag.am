check_bin_langu = check/bin/langu
check_bin_langu_LDADD = \
  $(lib_libwant_langu_la) \
  -ltunitas \
  -lrigging-bench-module \
  $(end)
# Reminder: autotools does not put $(AM_LDFLAGS) into check_bin_langu_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_langu_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_langu_OBJECTS) : AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_langu_OBJECTS) : AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_langu_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_langu_SOURCES))
check_bin_langu_SOURCES = \
  obj/src/tests/langu/main.cpp \
  obj/src/tests/langu/namespace.cpp \
  obj/src/tests/langu/age/namespace.cpp \
  obj/src/tests/langu/ish/namespace.cpp \
  \
  obj/src/tests/langu/age/Fixture.cpp \
  obj/src/tests/langu/age/format.Fixture.cpp \
  obj/src/tests/langu/age/format.dq.Fixture.cpp \
  \
  obj/src/tests/langu/ish/Fixture.cpp \
  obj/src/tests/langu/ish/python.binascii.Fixture.cpp \
  $(end)
$(check_bin_langu_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_DISAGGREGATE, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/langu/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $(check_bin_langu_SOURCES))
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/langu/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/langu/dependencies.mk
